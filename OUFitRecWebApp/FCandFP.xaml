<Window x:Class="OUFitRecWebApp.FCandFP"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OUFitRecWebApp"
        mc:Ignorable="d"
        Title="FCandFP" Height="1380" Width="1280">
    <Grid>
        <Grid>
            <!-- FitPassPage Content -->
            <Grid x:Name="FitPassPageContent" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                <!-- Fit Pass Image -->

                <!-- Fit Pass Classes Text -->
                <Rectangle Height="100" Stroke="Black" Margin="154,1058,154,206" Fill="#CCC8C8C8"/>
                <Rectangle Height="100" Stroke="Black" Margin="154,958,154,306" Fill="#CCC8C8C8"/>
                <Rectangle Height="100" Stroke="Black" Margin="154,858,154,406" Fill="#CCC8C8C8"/>
                <Rectangle Height="100" Stroke="Black" Margin="154,758,154,506" Fill="#CCC8C8C8"/>
                <Rectangle Height="100" Stroke="Black" Margin="154,660,154,604" Fill="#CCC8C8C8"/>
                <Rectangle Height="100" Stroke="Black" Margin="154,560,154,704" Fill="#CCC8C8C8"/>
                <Rectangle Height="100" Stroke="Black" Margin="154,458,154,806" Fill="#CCC8C8C8"/>
                <!-- Fit Pass Classes Text -->
                <TextBlock x:Name="FitPassClassesText" Text="Fit Pass Classes" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="443,184,0,0" 
                       FontSize="58" FontWeight="Bold" Foreground="#161414" FontFamily="SourceSansPro"/>

                <!-- Select Time Text -->
                <ComboBox Margin="154,354,756,958"/>


                <!-- Time Slots and Reserve Spot Buttons -->
                <TextBlock x:Name="TimeSlot1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="265,475,0,0" 
                       FontSize="18" FontWeight="Bold" Foreground="White" FontFamily="SourceSansPro"/>
                <Button x:Name="FPB1" Content="Reserve Spot" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="548,475,0,0" 
                    Width="200" Height="50" FontSize="18" FontWeight="Bold" Background="#841617" Foreground="White" FontFamily="SourceSansPro"/>

                <TextBlock x:Name="TimeSlot2" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="265,575,0,0" 
                       FontSize="18" FontWeight="Bold" Foreground="White" FontFamily="SourceSansPro"/>
                <Button x:Name="FPB2" Content="Reserve Spot" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="548,575,0,0" 
                    Width="200" Height="50" FontSize="18" FontWeight="Bold" Background="#841617" Foreground="White" FontFamily="SourceSansPro"/>

                <TextBlock x:Name="TimeSlot3" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="265,675,0,0" 
                       FontSize="18" FontWeight="Bold" Foreground="White" FontFamily="SourceSansPro"/>
                <Button x:Name="FPB3" Content="Reserve Spot" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="548,675,0,0" 
                    Width="200" Height="50" FontSize="18" FontWeight="Bold" Background="#841617" Foreground="White" FontFamily="SourceSansPro"/>

                <TextBlock x:Name="TimeSlot4" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="265,775,0,0" 
                       FontSize="18" FontWeight="Bold" Foreground="White" FontFamily="SourceSansPro"/>
                <Button x:Name="FPB4" Content="Reserve Spot" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="548,775,0,0" 
                    Width="200" Height="50" FontSize="18" FontWeight="Bold" Background="#841617" Foreground="White" FontFamily="SourceSansPro"/>

                <TextBlock x:Name="TimeSlot5" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="265,875,0,0" 
                       FontSize="18" FontWeight="Bold" Foreground="White" FontFamily="SourceSansPro"/>
                <Button x:Name="FPB5" Content="Reserve Spot" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="548,875,0,0" 
                    Width="200" Height="50" FontSize="18" FontWeight="Bold" Background="#841617" Foreground="White" FontFamily="SourceSansPro"/>

                <TextBlock x:Name="TimeSlot6" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="265,975,0,0" 
                       FontSize="18" FontWeight="Bold" Foreground="White" FontFamily="SourceSansPro"/>
                <Button x:Name="FPB6" Content="Reserve Spot" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="548,975,0,0" 
                    Width="200" Height="50" FontSize="18" FontWeight="Bold" Background="#841617" Foreground="White" FontFamily="SourceSansPro"/>

                <TextBlock x:Name="TimeSlot7" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="265,1075,0,0" 
                       FontSize="18" FontWeight="Bold" Foreground="White" FontFamily="SourceSansPro"/>
                <Button x:Name="FPB7" Content="Reserve Spot" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="548,1075,0,0" 
                    Width="200" Height="50" FontSize="18" FontWeight="Bold" Background="#841617" Foreground="White" FontFamily="SourceSansPro"/>
            </Grid>

            <!-- FitPassPageNavTop -->
            <Grid x:Name="FitPassPageNavTop" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="90" Background="#FF841617">
                <!-- FCHomeIcon Button -->
                <Button x:Name="FCHomeIcon" Margin="40,9,1044,9" Foreground="#00000000" BorderBrush="#00707070" Click="FCHomeIcon_Click">
                    <Button.Background>
                        <ImageBrush ImageSource="/oufitreclogocrop.png"/>
                    </Button.Background>
                </Button>
                <!-- FCSIB Button -->
                <Button x:Name="HPSIB" Style="{StaticResource RectangleButtonStyle}" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="1100,0,0,0" Height="50" Width="115" Background="White" BorderBrush="White" Foreground="#00000000" Click="HPSIB_Click">
                    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <TextBlock x:Name="SignInText" Text="Sign In" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="24" FontWeight="Bold" Foreground="#841617" FontFamily="SourceSansPro"/>
                    </StackPanel>
                </Button>
            </Grid>

            <!-- FitPassPageBottomNav -->
            <Grid x:Name="FitPassPageBottomNav" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="90" Background="#FF841617">
                <!-- WhiteOULogo -->
                <Image x:Name="HPWhiteOULogo" Source="/whiteoulogo (1).png" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="60,5,0,0" Height="79" Width="73"/>

                <!-- Fitness + Recreation Text -->
                <TextBlock x:Name="FitnessText" Text="Fitness + Recreation" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="150,5,0,0" 
           FontSize="18" FontWeight="Bold" Foreground="White" FontFamily="SourceSansPro"/>

                <!-- Address Text -->
                <TextBlock x:Name="AddressText" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="150,30,0,0" 
            FontSize="16" Foreground="White" FontFamily="SourceSansPro">
     <Run Text="1401 Asp Avenue, Room 170"/>
     <LineBreak/>
     <Run Text="Norman, OK 73019"/>
     <LineBreak/>
     <Run Text="(405) 325-0311"/>
                </TextBlock>
            </Grid>
        </Grid>
    </Grid>
</Window>
