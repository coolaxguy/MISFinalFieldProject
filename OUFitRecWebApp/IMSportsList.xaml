<Window x:Class="OUFitRecWebApp.IMSportsList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OUFitRecWebApp"
        mc:Ignorable="d"
        Title="IMSportsList" Height="830" Width="1280">
    <Grid>
        <!-- IMPageNavTop -->
        <Grid x:Name="IMPageNavTop" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="90" Background="#FF841617">
            <!-- IMHomeIcon Button -->
            <Button x:Name="IMLHomeIcon" Margin="40,9,1044,9" Foreground="#00000000" BorderBrush="#00707070" Click="IMLHomeIcon_Click">
                <Button.Background>
                    <ImageBrush ImageSource="/oufitreclogocrop.png"/>
                </Button.Background>
            </Button>
            <!-- IMSIB Button -->
            <Button x:Name="IMSIB" Style="{StaticResource RectangleButtonStyle}" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="1100,0,0,0" Height="50" Width="115" Background="White" BorderBrush="White" Foreground="#00000000" Click="IMSIB_Click">
                <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
                    <TextBlock x:Name="SignInText" Text="Sign In" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="24" FontWeight="Bold" Foreground="#841617" FontFamily="SourceSansPro"/>
                </StackPanel>
            </Button>
        </Grid>
        <!-- IMPageBottomNav -->
        <Grid x:Name="IMPageBottomNav" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="90" Background="#FF841617">
            <!-- WhiteOULogo -->
            <Image x:Name="HPWhiteOULogo" Source="/whiteoulogo (1).png" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="60,5,0,0" Height="79" Width="73"/>

            <!-- Fitness + Recreation Text -->
            <TextBlock x:Name="FitnessText" Text="Fitness + Recreation" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="150,5,0,0" 
       FontSize="18" FontWeight="Bold" Foreground="White" FontFamily="SourceSansPro"/>

            <!-- Address Text -->
            <TextBlock x:Name="AddressText" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="150,30,0,0" 
       FontSize="16" Foreground="White" FontFamily="SourceSansPro">
<Run Text="1401 Asp Avenue, Room 170"/>
<LineBreak/>
<Run Text="Norman, OK 73019"/>
<LineBreak/>
<Run Text="(405) 325-0311"/>
            </TextBlock>
        </Grid>
        <Rectangle HorizontalAlignment="Left" Height="160" Margin="31,187,0,0" Stroke="Black" VerticalAlignment="Top" Width="406"/>
        <Rectangle HorizontalAlignment="Center" Height="160" Margin="0,187,0,0" Stroke="Black" VerticalAlignment="Top" Width="406"/>
        <Rectangle HorizontalAlignment="Left" Height="160" Margin="843,187,0,0" Stroke="Black" VerticalAlignment="Top" Width="406"/>
        <Rectangle HorizontalAlignment="Left" Height="160" Margin="31,347,0,0" Stroke="Black" VerticalAlignment="Top" Width="406"/>
        <Rectangle HorizontalAlignment="Center" Height="160" Margin="0,347,0,0" Stroke="Black" VerticalAlignment="Top" Width="406"/>
        <Rectangle HorizontalAlignment="Left" Height="160" Margin="843,347,0,0" Stroke="Black" VerticalAlignment="Top" Width="406"/>
        <Rectangle HorizontalAlignment="Left" Height="160" Margin="31,507,0,0" Stroke="Black" VerticalAlignment="Top" Width="406"/>
        <Rectangle HorizontalAlignment="Center" Height="160" Margin="0,507,0,0" Stroke="Black" VerticalAlignment="Top" Width="406"/>
        <Rectangle HorizontalAlignment="Left" Height="160" Margin="843,507,0,0" Stroke="Black" VerticalAlignment="Top" Width="406"/>
        <TextBlock x:Name="IM1" HorizontalAlignment="Left" Height="155" Margin="31,187,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="401" FontSize="20"/>
        <TextBlock x:Name="IM2" HorizontalAlignment="Left" Height="155" Margin="442,187,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="401" FontSize="20"/>
        <TextBlock x:Name="IM3" HorizontalAlignment="Left" Height="155" Margin="848,187,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="401" FontSize="20"/>
        <TextBlock x:Name="IM4" HorizontalAlignment="Left" Height="155" Margin="31,350,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="401" FontSize="20"/>
        <TextBlock x:Name="IM5" HorizontalAlignment="Left" Height="155" Margin="442,352,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="401" FontSize="20"/>
        <TextBlock x:Name="IM6" HorizontalAlignment="Left" Height="155" Margin="848,347,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="401" FontSize="20"/>
        <TextBlock x:Name="IM7" HorizontalAlignment="Left" Height="154" Margin="36,510,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="401" FontSize="20"/>
        <TextBlock x:Name="IM8" HorizontalAlignment="Left" Height="155" Margin="442,512,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="401" FontSize="20"/>
        <TextBlock x:Name="IM9" HorizontalAlignment="Left" Height="155" Margin="848,510,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="401" FontSize="20"/>
        <TextBlock HorizontalAlignment="Left" Height="67" Margin="174,95,0,0" TextWrapping="Wrap" Text="IM Sport List" VerticalAlignment="Top" Width="938" FontSize="48" FontWeight="Bold" TextAlignment="Center"/>
        <Button Content="Return to IM Page" HorizontalAlignment="Left" Height="34" Margin="31,107,0,0" VerticalAlignment="Top" Width="172" Foreground="White" BorderBrush="#FF841617" Background="#FF841617" FontSize="18" FontWeight="Bold" Click="Button_Click"/>
    </Grid>
</Window>
